{
  "Const Variable": {
    "prefix": "cv",
    "body": [
      "const ${1:name} = ${0};"
    ],
    "description": "Const"
  },
  "Let Variable": {
    "prefix": "lv",
    "body": [
      "let ${1:name} = ${0};"
    ],
    "description": "Let Variable"
  },
  "Named function": {
    "prefix": "fun",
    "body": [
      "function ${1:name}(${2}){",
      "${0}",
      "}"
    ],
    "description": "Named function"
  },
  "Arrow Function": {
    "prefix": "afun",
    "body": [
      "(${1}) => {${0}}"
    ],
    "description": "Arrow Function"
  },
